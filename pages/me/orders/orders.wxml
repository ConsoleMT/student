<block wx:for="{{list}}">
  <view class="order-wrap" bindtap="toClassDetail" data-id="{{item.classId}}">
    <view>订单号  : {{item.orderNum}}</view>
    <view>下单日期: {{item.time}}</view>
    <view>课程名称: <text class="buy-name">{{item.className}}</text></view>
    <view>支付金额: {{item.payMoney}}元</view>
  </view>
</block>